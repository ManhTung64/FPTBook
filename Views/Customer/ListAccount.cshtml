@using Microsoft.AspNetCore.Identity
@if(Model.Count != 0)
{
    @foreach (var i in Model)
    {
        @i.UserName
        @i.PasswordHash
        @i.Id
            <form asp-controller="Customer" asp-action="ChangePassword" method="post">
                <input name="id" value="@i.Id" type="hidden">
                         Change pass: <input type="password" name="pass"/>
                 <input type="submit" value="Change"/>
            </form>
    }
}
else{
    <span>@TempData["message"]</span>
    
}
